params {

  // Main arguments
  reads = 'https://lifebit-featured-datasets.s3-eu-west-1.amazonaws.com/pipelines/rmats/reads/replicates/paired_end_reads_replicates.csv'
  singleEnd = false

  // Reads
  stranded = false
  b1 = 'https://lifebit-featured-datasets.s3-eu-west-1.amazonaws.com/pipelines/rmats/reads/replicates/b1.txt'
  b2 = 'https://lifebit-featured-datasets.s3-eu-west-1.amazonaws.com/pipelines/rmats/reads/replicates/b2.txt'
  adapter = 'NO_FILE'
  readlength = 48
  overhang = 100
  mismatch = 2

  // Genome references
  // star_index = 's3://ngi-igenomes/igenomes/Homo_sapiens/NCBI/GRCh38/Sequence/STARIndex/'
  // gtf = 'https://lifebit-featured-datasets.s3-eu-west-1.amazonaws.com/projects/jax/splicing-pipelines-nf/references/GRCh38/gencode.v32.primary_assembly.annotation.gtf'

  star_index  = '/projects/adeslat/igenomes/Homo_sapiens/NCBI/GRCh38/Sequence/STARIndex'
  gtf         = '/projects/adeslat/igenomes/Homo_sapiens/Gencode/gencode.v32.primary_assembly.annotation.gtf'

  // Other
  skiprMATS = false
  skipMultiQC = false
  outdir = 'results'

  // Resources
  max_cpus = 16
  max_memory = '120.GB'

}